import java.util.List;

public class Node {
	private String name;
	private Node parent;
	private List<Node> children; 
	
	public Node(String name){
		this.name = name; 
	}
	public Node(String name, Node parent){
		this.name = name; 
		this.parent = parent; 
	}
	public String getName(){
		return name;
	}
	
}
